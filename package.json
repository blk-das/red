{
  "name": "social-media-frontend",
  "version": "1.0.0",
  "description": "A social media proyect in React with Webpack",
  "main": "./src/index.js",
  "scripts": {
    "build": "webpack --mode=production --env=production",
    "dev": "webpack serve --config ./webpack.config.dev.js --mode=development --env file=development",
    "test": "jest --env=jsdom --watch",
    "test:coverage": "jest --env=jsdom --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathangg03/social-media-frontend.git"
  },
  "author": "Jonathan Garcia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonathangg03/social-media-frontend/issues"
  },
  "homepage": "https://github.com/jonathangg03/social-media-frontend#readme",
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "axios": "^0.22.0",
    "dotenv": "^16.0.0",
    "dotenv-webpack": "^7.1.0",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.0",
    "eslint-plugin-react": "^7.26.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.4.3",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "jest": {
    "moduleNameMapper": {
