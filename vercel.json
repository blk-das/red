{
  "routes": [
    {
      "src": "(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "(.*).json",
      "dest": "/$1.json"
    },

    {
      "src": "(.*).png",
      "dest": "/$1.png"
    },
    {
      "src": "(.*).jpg",
      "dest": "/$1.jpg"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
